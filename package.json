  "dependencies": {    
    "netlify-lambda": "^2.0.3",
    "npm-run-all": "^4.1.5"
  },
  "scripts": {
    "devold": "netlify-lambda serve functions",
    "start:lambda": "netlify-lambda serve --config ./webpack.functions.due.js functions",
    "build-externals": "netlify-lambda build --config ./webpack.functions.js functions",
    "build-lambda": "netlify-lambda build --config ./webpack.functions.due.js functions",
    "build": "NODE_ENV=production npm-run-all build:*",
    "build:functions-install-dependencies": "cd functions; for i in */; do cd ${i%/}; yarn; cd ..; done;",
    "build:zip-functions": "cd functions; for i in */; do zip -r \"../functions/${i%/}.zip\" \"$i\"; done;",
    "build:copy-single-functions": "cp ./functions/*.js ./functions"
  },
  "devDependencies": {
    "webpack-node-externals": "^2.5.2"
  }
}
